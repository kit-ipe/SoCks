# openafs.repo

[openafs]
name=openafs-$releasever
baseurl=http://linuxsoft.cern.ch/cern/alma/$releasever/openafs/$basearch
gpgcheck=1
enabled=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-kojiv2

[openafs-testing]
name=openafs-$releasever - testing
baseurl=http://linuxsoft.cern.ch/cern/alma/$releasever/openafs/$basearch
gpgcheck=1
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-kojiv2
