From fded83e2d293b165d4ba2f179a4b5bffebb79c1d Mon Sep 17 00:00:00 2001
From: "marvin.fuchs" <marvin.fuchs@cern.ch>
Date: Wed, 3 Jul 2024 15:57:43 +0200
Subject: [PATCH] Set MAC address offset for Kria EEPROM

---
 .config | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/.config b/.config
index 946c9f9a3c..414d282344 100644
--- a/.config
+++ b/.config
@@ -4,7 +4,7 @@
 #
 
 #
-# Compiler: aarch64-linux-gnu-gcc (GCC) 12.1.1 20220507 (Red Hat Cross 12.1.1-2)
+# Compiler: aarch64-linux-gnu-gcc (GCC) 12.1.1 20220507 (Red Hat Cross 12.1.1-1)
 #
 CONFIG_CREATE_ARCH_SYMLINK=y
 CONFIG_SYS_CACHE_SHIFT_6=y
@@ -214,7 +214,7 @@ CONFIG_XILINX_PS_INIT_FILE=""
 CONFIG_XILINX_OF_BOARD_DTB_ADDR=0x100000
 CONFIG_BOOT_SCRIPT_OFFSET=0x3E80000
 CONFIG_ZYNQ_MAC_IN_EEPROM=y
-CONFIG_ZYNQ_GEM_I2C_MAC_OFFSET=0x20
+CONFIG_ZYNQ_GEM_I2C_MAC_OFFSET=0x83
 CONFIG_CMD_FRU=y
 # CONFIG_SYS_MEM_RSVD_FOR_MMU is not set
 CONFIG_PMUFW_INIT_FILE=""
-- 
